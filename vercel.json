{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "installCommand": "npm install --legacy-peer-deps",
        "buildCommand": "DISABLE_ESLINT_PLUGIN=true CI=false npm run build"
      }
    }
  ],
  "env": {
    "DISABLE_ESLINT_PLUGIN": "true",
    "CI": "false"
  }
}
